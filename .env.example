# .env.example
# This is an example file for environment variables.
# Copy this file to .env and fill in the actual values.
# Do NOT commit the .env file to version control.

# PostgreSQL Database Configuration
# These are used by both the 'postgres' and 'app' services in docker-compose.
POSTGRES_DB=payment_tracker
POSTGRES_USER=payment_tracker_user
POSTGRES_PASSWORD=your_strong_db_password

# The application uses this variable to connect to the database.
# It should be the same as POSTGRES_PASSWORD.
DB_PASSWORD=your_strong_db_password

# RocketMQ Configuration
ROCKETMQ_NAME_SERVER=localhost:9876

# OpenAI API Key for AI Analysis Service
OPENAI_API_KEY=your_openai_api_key

# Aliyun OSS Configuration for file storage
ALIYUN_OSS_ACCESS_KEY=your_aliyun_oss_access_key
ALIYUN_OSS_SECRET_KEY=your_aliyun_oss_secret_key